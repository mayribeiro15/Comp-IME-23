	<queryString language="SQL">
		<![CDATA[select alteracaoEquipamento.nome  as nome,
 ifnull(alteracaoEquipamento.serie, '-') as serie,
 ifnull(alteracaoEquipamento.arma, '-' ) as arma,
 ifnull(alteracaoEquipamento.fabricante, '-') as fabricante,
 ifnull(alteracaoEquipamento.calibre, '-') as calibre,
 alteracaoEquipamento.dataAlteracao as data,
 alteracaoEquipamento.categoria,
 alteracaoEquipamento.statusEqp as status,
 militar.login as armeiro, of.login as oficial
 from alteracaoEquipamento
inner join funcaomilitar on funcaomilitar.id = alteracaoEquipamento.idArmeiro
left join militar  on militar.id = funcaomilitar.idMilitar
inner join funcaomilitar oficial on oficial.id = alteracaoEquipamento.idOficialDia
left join militar of  on of.id = oficial.idMilitar]]>
	</queryString>
