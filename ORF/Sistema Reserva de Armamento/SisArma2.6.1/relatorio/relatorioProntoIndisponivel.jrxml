	<queryString language="SQL">
		<![CDATA[select 
	concat(
	if(a.nome is null,equipamento.nome,a.nome),	
	ifnull(a.nome, ''))  as nome,
	ifnull(c.nome, '-') as calibre,
	ifnull(f.nome, '-') as fabricante,
	sum(equipamento.qtd )	as qtd
	from equipamento
	left join arma a on a.id = equipamento.idArma
	left join fabricante f on f.id = equipamento.idFabricante
	left join calibre c on c.id = equipamento.idCalibre
	where equipamento.statusEqp = 'INDISPONIVEL' group by equipamento.nome, a.nome;]]>
	</queryString>
